{
  "name": "CHARTextract",
  "version": "0.3.2",
  "private": true,
  "main": "bin/www",
  "author": "LKS-CHART",
  "description": "Tool for writing and refining rules for information extraction.",
  "scripts": {
    "start": "node ./bin/www",
    "start-electron": "electron ./bin/www",
    "pack": "build --dir",
    "dist": "build"
  },
  "dependencies": {
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "http-errors": "~1.6.2",
    "morgan": "~1.9.0",
    "pug": "2.0.0-beta11",
    "python-shell": "^0.5.0",
    "table-parser": "^1.0.1",
    "windows-autoconf": "^1.11.1",
    "electron-localshortcut": "^3.1.0",
    "electron-is-dev": "^1.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "electron": "9.1.0",
    "electron-builder": "^20.33.2",
    "eslint": "^5.8.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "semantic-release": "^15.10.8"
  },
  "build": {
    "appId": "com.lks-chart.chart-extract",
    "asar": false,
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!**/dictionaries",
      "icon.png"
    ],
    "extraFiles": [
      {
        "from": "../RegexNLP-py/dictionaries",
        "to": "dictionaries"
      }
    ],
    "directories": {
      "output": "../Release"
    }
  },
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  }
}
